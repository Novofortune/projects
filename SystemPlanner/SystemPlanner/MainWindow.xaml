<Window x:Class="SystemPlanner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <GroupBox x:Name="Grid" Header="GroupBox" Margin="-3,6,33,68" RenderTransformOrigin="0.5,0.5">
        <GroupBox.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="1.587"/>
                <TranslateTransform/>
            </TransformGroup>
        </GroupBox.RenderTransform>
        <Grid x:Name="Grid2" HorizontalAlignment="Left" Height="280" Margin="25,10,0,0" VerticalAlignment="Top" Width="454">
            <Viewbox HorizontalAlignment="Left" Height="243" Margin="50,37,0,0" VerticalAlignment="Top" Width="375"/>
        </Grid>
    </GroupBox>
</Window>
